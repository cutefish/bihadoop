<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>

<!-- Put site-specific property overrides in this file. -->

<configuration>
    <!-- sim system -->
    <property>
        <name>node.schedule.choice</name>
        <value>0</value>
    </property>
    <property>
        <name>num.distributed.node</name>
        <value>16</value>
    </property>
    <property>
        <name>num.replicas</name>
        <value>3</value>
    </property>
    <property>
        <name>node.block.len</name>
        <value>64</value>
    </property>
    <property>
        <name>node.disk.capacity</name>
        <value>1024</value>
    </property>
    <property>
        <name>node.memory.capacity</name>
        <value>256</value>
    </property>
    <!-- job conf -->
    <property>
        <name>job.client.num.iterations</name>
        <value>1</value>
    </property>
    <property>
        <name>job.split.length.rand</name>
        <value>0.1</value>
    </property>
    <property>
        <name>job.class.name</name>
        <value>TestScheduler$AllPair</value>
    </property>
    <!-- all pair -->
    <property>
        <name>allpair.input0.length</name>
        <value>10240</value>
        <description>in MByte</description>
    </property>
    <property>
        <name>allpair.input1.length</name>
        <value>10240</value>
    </property>
    <property>
        <name>allpair.split.length</name>
        <value>128</value>
    </property>
    <!-- half all pair-->
    <property>
        <name>halfallpair.input.length</name>
        <value>10240</value>
        <description>in MByte</description>
    </property>
    <property>
        <name>halfallpair.split.length</name>
        <value>128</value>
    </property>
    <!-- diag block -->
    <property>
        <name>diagblock.input0.length</name>
        <value>10240</value>
        <description>in MByte</description>
    </property>
    <property>
        <name>diagblock.input1.length</name>
        <value>10240</value>
        <description>in MByte</description>
    </property>
    <property>
        <name>diagblock.split0.length</name>
        <value>128</value>
    </property>
    <property>
        <name>diagblock.split1.length</name>
        <value>128</value>
    </property>
    <property>
        <name>diagblock.num.blocks</name>
        <value>16</value>
    </property>
    <!-- rand pair -->
    <property>
        <name>randpair.dense.level</name>
        <value>0.2</value>
    </property>
    <property>
        <name>randpair.input0.length</name>
        <value>10240</value>
    </property>
    <property>
        <name>randpair.input1.length</name>
        <value>10240</value>
    </property>
    <property>
        <name>randpair.split.length</name>
        <value>128</value>
    </property>
</configuration>
